@section Scripts{

<script type="text/javascript">

    $('#teste').modal('show');
    $("#confirme").modal("show")
    $("#modal-email").modal("show")


    $("#enviar").click(function () {
        $("#Form").submit();
        var erros = $("body").find(".field-validation-error");
        if (erros.length == 0) {
            $(this).attr("disabled", true)
            $("input").attr("readonly", true);
            $("#icon").addClass("fa fa-spinner fa-spin");
        }
    });



    function verificaCPF() {
        var cpf = $("#CPF").val()
        if (cpf.length >= 12) {
            $.get('@Url.Action("VerificaCPF")', { cpf }, function (response) {
                if (response != 0) {
                    $("#isExist").html("  CPF já cadastrado. <span id='openModal' style='color:#4285F4; cursor:pointer' data-id='" + response.Id + "' data-cpf='" + response.CPF + "' ' >Clique aqui para verificar sua matrícula. </span>")
                    $("#enviar").attr("disabled", true)
                    console.log(response)    
                    $("#openModal").click(function () {
                $(".numero-modal").text(response.Numero)
                $(".nome-modal").text(response.Nome)
                $(".CPF-modal").text(response.CPF)
                $(".sexo-modal").text(response.Sexo)
                $(".camiseta-modal").text(response.Camiseta)
                $(".cidade-modal").text(response.Cidade)
                $(".endereco-modal").text(response.Endereco)
                $(".email-modal").text(response.Email)
                var tipo = response.TipoProva == 1 ? "Corrida - 5KM" : "Caminhada - 3KM"
                $(".tipo-modal").text(tipo),
                $(".data-modal").text(response.DataNasc)
                var elite = response.IsElite ? "Sim" : "Não"
                $(".elite-modal").text(elite)
                $(".telefone-modal").text(response.Telefone)
                $(".cancelar-modal-link").html("<a href='https://devroyale.com/Cancelar?id=" + response.Id + "' >CLIQUE AQUI</a>")
                $(".email-modal-link").html("<a href='https://devroyale.com/Home/EmailSend?id=" + response.Id + "' >CLIQUE AQUI</a>")
                $("#teste2").modal("show")
            })
                }
            },"json");
        } else {
            
            $("#isExist").html("");
            $("#enviar").removeAttr("disabled", true)
        }
    }



    $("#CPF").keyup(function () {
        $("#isExist").html("");
        verificaCPF();
    })

    $("#CPF").focusin(function () {
        $("#isExist").html("");
        verificaCPF();
    })

    $("#CPF").focusout(function () {

        setTimeout(function () {
            $("#isExist").html("");
            verificaCPF();
        }, 100)

    })

    $("#CPF").change(function () {
        verificaCPF();
    })

    $("#CPF").mask("000.000.000-00")

    $("#Telefone").mask("(00) 0 0000-0000")

    $("#DataNasc").mask("00/00/0000")
    
    $(function () {
        $('[data-toggle="tooltip"]').tooltip()
    })

    $(".prova").change(function () {
        if ($(this).val() == "1") {
            $(".mostrar").removeClass("d-none");
        } else {
            $(".mostrar").addClass("d-none");

        }
    })


    </script>

}