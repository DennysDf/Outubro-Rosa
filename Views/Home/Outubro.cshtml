<section id="contact">
    <div class="container">
        @*@if (DateTime.Now >= DateTime.Parse("9/28/2019 11:59:59"))*@

        @if (true)
        {
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading text-uppercase">QUERO PARTICIPAR</h2>
                <h3 class="section-subheading text-muted">Formulário para participar</h3>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12">
                <form asp-action="Cadastrar" id="Form">
                    <div class="row">
                        <div class="col-12 text-center  text-white-50">
                            Informações do Participante
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <input class="form-control focus" asp-for="Nome" type="text" placeholder="Nome completo *">
                                <span asp-validation-for="Nome"></span>
                            </div>
                            <div class="form-group">
                                <input class="form-control focus" asp-for="Endereco" type="text" placeholder="Endereço completo *">
                                <span asp-validation-for="Endereco"></span>
                            </div>
                            <div class="form-group">
                                <input class="form-control focus" asp-for="Telefone" type="text" placeholder="Telefone celular *">
                                <span asp-validation-for="Telefone"></span>
                            </div>
                            <div class="form-group text-white">
                                <input class="form-control focus" asp-for="DataNasc" type="text" placeholder="Data de Nascimento *">
                                <span asp-validation-for="DataNasc"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <input class="form-control focus" type="text" asp-for="CPF" placeholder="CPF *">
                                <span style="color:red;" id="isExist"></span>
                                <span asp-validation-for="CPF"></span>
                            </div>
                            <div class="form-group">
                                <input class="form-control focus" asp-for="Email" type="text" placeholder="Seu e-mail *">
                                <span asp-validation-for="Email"></span>
                            </div>

                            <div class="form-group">
                                <input class="form-control focus" asp-for="Cidade" type="text" placeholder="Cidade *">
                                <span asp-validation-for="Cidade"></span>
                            </div>

                            <div class="form-group text-white">
                                <label asp-for="Sexo" id="M" class="bottom"><input asp-for="Sexo" type="radio" value="M" /> Masculino</label>
                                <label asp-for="Sexo" id="F" class="bottom"><input asp-for="Sexo" type="radio" value="F" /> Feminino</label>

                                <div class="form-group">
                                    <span asp-validation-for="Sexo"></span>
                                </div>

                            </div>

                        </div>
                    </div>

                    <div class="row">

                        <div class="col-12 text-center text-white-50">
                            Informações da prova
                        </div>

                        <div class="col-md-6">
                            <div class="form-group text-white">
                                <label class="bottom "><input asp-for="TipoProva" id="1" class="prova" type="radio" value="1" /> Corrida - 5KM</label>
                                <div class="mostrar d-none" id="alerta">
                                    <label class="text-white">
                                        <input type="checkbox" asp-for="IsElite" /> <span class="font-weight-bold">Deseja fazer parte do grupo de elite? </span>
                                    </label>
                                    <i class="fa fa-info-circle" style="cursor:pointer;" data-toggle="tooltip" title="A categoria elite é recomendada para atletas que praticam treinamento especifico para corrida."></i>
                                </div>
                                <label class="bottom"><input class="prova" type="radio" asp-for="TipoProva" id="2" value="2" /> Caminhada - 3KM</label>
                                <div class="form-group"><span asp-validation-for="TipoProva"></span></div>
                            </div>
                        </div>

                        <div class="col-md-6">
                            <div class="form-group text-white text-center">
                                <span class=""> Camiseta</span><br />
                                <label><input asp-for="Camiseta" type="radio" id="p" value="P" /> P</label>
                                <label><input asp-for="Camiseta" type="radio" id="m" value="M" /> M</label>
                                <label><input asp-for="Camiseta" type="radio" id="g" value="G" /> G</label>
                                <label><input asp-for="Camiseta" type="radio" id="gg" value="GG" /> GG</label>

                                <div class="form-group">
                                    <span asp-validation-for="Camiseta"></span>
                                </div>

                            </div>

                        </div>
                    </div>
                    <div class="col-lg-12 text-center">
                        <div id="success"></div>
                        <button style="border: 1px solid #FEC810;" class="btn btn-primary btn-xl text-uppercase" id="enviar" type="submit"> <i class="" id="icon"></i> CADASTRAR</button>
                    </div>
                </form>
