                        <li class="list-inline-item" id="facebookhover">
                            <a target="_blank" href="https://www.facebook.com/dennys.fonseca.52">
                                <i class="fab fa-facebook-f"></i>
                            </a>
                        </li>
                        <li class="list-inline-item" id="instagram">
                            <a target="_blank" href="https://instagram.com/dennysfonseca_?igshid=1fsvro7n57uji">
                                <i class="fab fa-instagram"></i>
                            </a>
                        </li>
                    </ul>
                </div>

            </div>
        </div>
    </footer>

    <script src="~/vendor/jquery/jquery.min.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
